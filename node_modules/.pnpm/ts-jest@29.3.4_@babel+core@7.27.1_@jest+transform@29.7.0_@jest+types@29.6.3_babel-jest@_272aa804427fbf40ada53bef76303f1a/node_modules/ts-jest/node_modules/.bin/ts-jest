#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/betatable/Desktop/beastmode/opaque-urls-v01/node_modules/.pnpm/ts-jest@29.3.4_@babel+core@7.27.1_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_272aa804427fbf40ada53bef76303f1a/node_modules/ts-jest/node_modules:/Users/betatable/Desktop/beastmode/opaque-urls-v01/node_modules/.pnpm/ts-jest@29.3.4_@babel+core@7.27.1_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_272aa804427fbf40ada53bef76303f1a/node_modules:/Users/betatable/Desktop/beastmode/opaque-urls-v01/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/betatable/Desktop/beastmode/opaque-urls-v01/node_modules/.pnpm/ts-jest@29.3.4_@babel+core@7.27.1_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_272aa804427fbf40ada53bef76303f1a/node_modules/ts-jest/node_modules:/Users/betatable/Desktop/beastmode/opaque-urls-v01/node_modules/.pnpm/ts-jest@29.3.4_@babel+core@7.27.1_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_272aa804427fbf40ada53bef76303f1a/node_modules:/Users/betatable/Desktop/beastmode/opaque-urls-v01/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
